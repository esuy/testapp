	function trace(text,clear){text+='<br />';if(typeof this.inited=='undefined'){this.inited=true;var div=document.createElement("DIV");div.id="trace";div.style.cssText='max-width:600px;font-size:12px;white-space:pre;position:absolute;z-index:1000;font-family:monospace;top:3px;right:3px;border:solid 5px #B7BEC4;background-color:#E7ECF0;padding:5px;';document.body.insertBefore(div,document.body.firstChild);}if(clear)document.getElementById("trace").innerHTML=text;else document.getElementById("trace").innerHTML+=text;}
	function print_r(trg,ret){var data='';for(i in trg){data+=i+' => '+trg[i]+'<br />';}if(ret){return data;}else{trace(data,1);}}
	

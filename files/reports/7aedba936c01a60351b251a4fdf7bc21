--
-- Table structure for table `clients`
--

CREATE TABLE IF NOT EXISTS `clients` (
  `client_id` int(10) NOT NULL AUTO_INCREMENT,
  `client_module` int(10) NOT NULL DEFAULT '0',
  `client_adm_no` varchar(50) DEFAULT NULL,
  `client_entry_date` date DEFAULT NULL,
  `client_first_name` varchar(100) DEFAULT '',
  `client_other_name` varchar(100) DEFAULT '',
  `client_last_name` varchar(100) DEFAULT '',
  `client_notes` text,
  `client_status` int(3) NOT NULL DEFAULT '0',
  `client_custom` longtext,
  `client_gender` enum('1','2') DEFAULT NULL,
  `client_dob` date DEFAULT NULL,
  `client_doa` date DEFAULT NULL,
  `client_center` int(3) DEFAULT NULL,
  `client_nhif` varchar(100) DEFAULT NULL,
  `client_immun` varchar(100) DEFAULT NULL,
  `client_nhif_n` varchar(100) DEFAULT NULL,
  `client_immun_n` varchar(100) DEFAULT NULL,
  `client_lvd` date DEFAULT NULL,
  `client_lvd_form` varchar(50) DEFAULT NULL,
  `client_obsolete` enum('0','1') NOT NULL DEFAULT '0',
  PRIMARY KEY (`client_id`),
  UNIQUE KEY `client_id` (`client_id`),
  KEY `client_adm_no` (`client_id`,`client_adm_no`),
  KEY `update_index` (`client_adm_no`,`client_first_name`,`client_other_name`,`client_last_name`)
) ENGINE=MyISAM  DEFAULT CHARSET=latin1 AUTO_INCREMENT=1888 ;


  KEY `social_staff_name` (`social_id`,`social_staff_name`),
  KEY `social_client` (`social_id`,`social_client_id`),
  KEY `social_adm_no` (`social_id`,`client_adm_no`),
  KEY `social_staff` (`social_id`,`social_staff_id`),
  KEY `social_clinic` (`social_id`,`social_clinic_id`),
  KEY `social_client_adm_no` (`social_client_id`,`client_adm_no`),
  KEY `client_staff_id` (`social_client_id`,`social_staff_id`),
  KEY `client_clinic` (`social_client_id`,`social_clinic_id`),
  KEY `adm_no_staff_id` (`client_adm_no`,`social_staff_id`),
  KEY `adm_no_clinic` (`client_adm_no`,`social_clinic_id`),
  KEY `staff_id_clinic` (`social_staff_id`,`social_clinic_id`)
) ENGINE=InnoDB DEFAULT CHARSET=latin1;


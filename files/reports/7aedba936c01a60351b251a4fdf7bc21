  `caregiver_id` int(10) NOT NULL AUTO_INCREMENT,
  `caregiver_client_id` int(10) NOT NULL DEFAULT '0',
  `caregiver_client_caregiver_type` varchar(50) DEFAULT NULL,
  `caregiver_entry_date` date DEFAULT NULL,
  `caregiver_fname` varchar(100) DEFAULT '',
  `caregiver_lname` varchar(100) DEFAULT '',
  `caregiver_age` varchar(100) DEFAULT '',
  `caregiver_status` int(11) DEFAULT NULL,
  `caregiver_health_status` int(11) DEFAULT NULL,
  `caregiver_raising_child` int(11) DEFAULT NULL,
  `caregiver_marital_status` int(11) DEFAULT NULL,
  `caregiver_educ_level` int(11) DEFAULT NULL,
  `caregiver_employment` int(11) DEFAULT NULL,
  `caregiver_income` int(11) DEFAULT NULL,
  `caregiver_idno` varchar(100) DEFAULT '',
  `caregiver_mobile` varchar(100) DEFAULT '',
  `caregiver_notes` text,
  `caregiver_custom` longtext,
  `caregiver_role` enum('pri','sec') NOT NULL DEFAULT 'pri',
  `caregiver_active` enum('0','1') NOT NULL DEFAULT '1',
  `caregiver_off` date DEFAULT NULL,
  PRIMARY KEY (`caregiver_id`)
) ENGINE=MyISAM  DEFAULT CHARSET=latin1 AUTO_INCREMENT=984 ;


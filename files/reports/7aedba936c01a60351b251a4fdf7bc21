  `clinical_bloodtest_cd4_percentage` int(10) DEFAULT NULL,
  `clinical_bloodtest_viral` varchar(150) DEFAULT NULL,
  `clinical_bloodtest_hb` varchar(150) DEFAULT NULL,
  `clinical_weight` int(10) DEFAULT NULL,
  `clinical_height` int(10) DEFAULT NULL,
  `clinical_zscore` varchar(150) DEFAULT NULL,
  `clinical_muac` varchar(150) DEFAULT NULL,
  `clinical_hc` varchar(150) DEFAULT NULL,
  `clinical_child_unwell` int(11) DEFAULT '2',
  `clinical_temp` varchar(150) DEFAULT NULL,
  `clinical_resp_rate` varchar(150) DEFAULT NULL,
  `clinical_heart_rate` varchar(150) DEFAULT NULL,
  `clinical_general` varchar(150) DEFAULT NULL,
  `clinical_mouth` varchar(150) DEFAULT NULL,
  `clinical_ears` varchar(150) DEFAULT NULL,
  `clinical_clubbing` varchar(150) DEFAULT NULL,
  `clinical_chest` varchar(150) DEFAULT NULL,
  `clinical_cardiovascular` varchar(150) DEFAULT NULL,
  `clinical_skin` varchar(150) DEFAULT NULL,
  `clinical_abdomen` varchar(150) DEFAULT NULL,
  `clinical_neurodevt` varchar(150) DEFAULT NULL,
  `clinical_oedema` int(11) DEFAULT NULL,
  `clinical_adherence` int(11) DEFAULT NULL,
  `clinical_adherence_notes` varchar(255) DEFAULT NULL,
  `clinical_diarrhoea_type` int(11) DEFAULT '0',
  `clinical_diarrhoea_desc` varchar(255) DEFAULT NULL,
  `clinical_dehydration` int(11) DEFAULT '0',
  `clinical_pneumonia` int(11) DEFAULT '0',
  `clinical_tb` int(11) DEFAULT '0',
  `clinical_tb_treatment_date` date DEFAULT NULL,
  `clinical_pulmonary` varchar(150) DEFAULT NULL,
  `clinical_discharging_ears` int(11) DEFAULT '0',
  `clinical_other_diagnoses` varchar(255) DEFAULT NULL,
  `clinical_malnutrition` int(11) DEFAULT '0',
  `clinical_growth` int(11) DEFAULT '1',
  `clinical_assessment_notes` varchar(255) DEFAULT NULL,
  `clinical_investigations` int(11) DEFAULT '1',
  `clinical_investigations_notes` varchar(255) DEFAULT NULL,
  `clinical_arv` int(11) DEFAULT '1',
  `clinical_arv_notes` varchar(255) DEFAULT NULL,
  `clinical_arv_start` varchar(150) DEFAULT NULL,
  `clinical_arv_reason` varchar(255) DEFAULT NULL,
  `clinical_tb_drugs` int(11) DEFAULT NULL,
  `clinical_tb_drugs_notes` varchar(255) DEFAULT NULL,
  `clinical_septrin` int(11) DEFAULT '0',
  `clinical_vitamins` int(11) DEFAULT '0',
  `clinical_drugs` varchar(255) DEFAULT NULL,
  `clinical_nutritional_support` int(11) DEFAULT '0',
  `clinical_nutritional_notes` varchar(255) DEFAULT NULL,
  `clinical_referral` varchar(150) DEFAULT NULL,
  `clinical_next_date` date DEFAULT NULL,
  `clinical_notes` text,
  `clinical_custom` longtext,
  PRIMARY KEY (`clinical_id`),
  UNIQUE KEY `clinical_id` (`clinical_id`),
  KEY `clinical_client_id` (`clinical_id`,`clinical_client_id`),
  KEY `clinical_clinic_id` (`clinical_id`,`clinical_clinic_id`),
  KEY `client_clinic_id` (`clinical_client_id`,`clinical_clinic_id`),
  KEY `clinical_caregiver_id` (`clinical_id`,`clinical_caregiver_id`),
  KEY `client_caregiver_id` (`clinical_client_id`,`clinical_caregiver_id`)
) ENGINE=InnoDB DEFAULT CHARSET=latin1 AUTO_INCREMENT=1 ;


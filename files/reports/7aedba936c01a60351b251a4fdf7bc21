--
-- Table structure for table `household_info`
--

CREATE TABLE IF NOT EXISTS `household_info` (
  `household_id` int(10) NOT NULL AUTO_INCREMENT,
  `household_client_id` int(10) NOT NULL,
  `household_admission_id` int(11) DEFAULT '0',
  `household_social_id` int(11) DEFAULT NULL,
  `household_name` varchar(150) DEFAULT NULL,
  `household_yob` varchar(150) DEFAULT NULL,
  `household_relationship` varchar(150) DEFAULT NULL,
  `household_gender` int(11) DEFAULT NULL,
  `household_notes` text,
  `household_custom` longtext,
  PRIMARY KEY (`household_id`),
  UNIQUE KEY `household_id` (`household_id`),
  KEY `household_client_id` (`household_id`,`household_client_id`),
  KEY `household_admission_id` (`household_id`,`household_admission_id`),
  KEY `household_social_id` (`household_id`,`household_social_id`),
  KEY `client_admission_id` (`household_client_id`,`household_admission_id`),
  KEY `client_social_id` (`household_client_id`,`household_social_id`),
  KEY `admission_social_id` (`household_admission_id`,`household_social_id`)
) ENGINE=InnoDB  DEFAULT CHARSET=latin1 AUTO_INCREMENT=8398 ;

-- --------------------------------------------------------

--
-- Table structure for table `household_staging`
--

CREATE TABLE IF NOT EXISTS `household_staging` (
  `household_id` int(10) DEFAULT NULL,
  `household_client_id` int(10) DEFAULT NULL,
  `client_adm_no` varchar(50) DEFAULT NULL,
  `client_first_name` varchar(100) DEFAULT '',
  `client_other_name` varchar(100) DEFAULT '',
  `client_last_name` varchar(100) DEFAULT '',
  `household_admission_id` int(11) DEFAULT '0',
  `household_social_id` int(11) DEFAULT NULL,
  `household_name` varchar(150) DEFAULT NULL,
  `household_yob` varchar(150) DEFAULT NULL,
  `household_relationship` varchar(150) DEFAULT NULL,
  `household_gender` int(11) DEFAULT NULL,
  `household_notes` text,
  `household_custom` longtext,
  KEY `household_client_id` (`household_id`,`household_client_id`),
  KEY `household_adm_no` (`household_id`,`client_adm_no`),
  KEY `household_adm_id` (`household_id`,`household_admission_id`),
  KEY `household_social_id` (`household_id`,`household_social_id`),
  KEY `client_adm_no` (`household_client_id`,`client_adm_no`),
  KEY `client_adm_id` (`household_client_id`,`household_admission_id`),
  KEY `client_social_id` (`household_client_id`,`household_social_id`),
  KEY `client_adm_adm_id` (`client_adm_no`,`household_admission_id`),
  KEY `client_adm_social_id` (`client_adm_no`,`household_social_id`),
  KEY `household_adm_id_social_id` (`household_admission_id`,`household_social_id`)
) ENGINE=InnoDB DEFAULT CHARSET=latin1;


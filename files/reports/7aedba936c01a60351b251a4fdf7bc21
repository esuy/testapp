  `counselling_positive_ref_notes` varchar(100) DEFAULT NULL,
  `counselling_admission_date` date DEFAULT NULL,
  `counselling_referral_source_notes` varchar(100) DEFAULT NULL,
  `counselling_referral_source_old` varchar(100) DEFAULT NULL,
  KEY `counselling_client` (`counselling_id`,`counselling_client_id`),
  KEY `counselling_adm_no` (`counselling_id`,`client_adm_no`),
  KEY `counselling_clinic` (`counselling_id`,`counselling_clinic`),
  KEY `counselling_staff_id` (`counselling_id`,`counselling_staff_id`),
  KEY `counselling_name` (`counselling_id`,`counselling_staff_name`)
) ENGINE=MyISAM DEFAULT CHARSET=latin1;


  `counselling_dis_couple` tinyint(1) unsigned DEFAULT NULL,
  `counselling_mother_treatment_where` varchar(100) DEFAULT NULL,
  `counselling_mother_pmtct_where` varchar(100) DEFAULT NULL,
  `counselling_mother_antenatal_where` varchar(100) DEFAULT NULL,
  `counselling_mother_cd4_note` varchar(50) DEFAULT NULL,
